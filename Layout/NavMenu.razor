@using CEJ_WebApp.Model
@inject UserSessionInformation UserSessionInformation

<MudNavMenu>
    <MudNavLink Href="/" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">Home</MudNavLink>
    <MudNavLink Href="/counter" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Counter</MudNavLink>
    <MudNavLink Href="/texto" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Texto</MudNavLink>
    @if (UserSessionInformation.Role != Model.Enum.UserRoleType.Operador)
    {
        <MudNavLink Href="/weather" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">Weather</MudNavLink>
    }
</MudNavMenu>


